local a=Instance.new("Tool")a.ToolTip="Generated"a.Name="Teleporter"a.RequiresHandle=false;a.Parent=script.Parent;local b=Instance.new("LocalScript")b.Parent=a;local c={}task.spawn(function()local script=b;local d=require;local function require(e)if c[e]then return c[e]()end;return d(e)end;local f=game.Players.LocalPlayer;local g=f.Character or f.CharacterAdded:Wait()local h=g:WaitForChild("Humanoid")local i,j,k={},{},{}local l=f:GetMouse()local function m()local n=l.Target;if n then local o=n:FindFirstAncestorWhichIsA("Model")if o and o.PrimaryPart then local p=o:FindFirstChildWhichIsA("Humanoid")local q=o.PrimaryPart;if p and p.Health>0 then if not p:GetAttribute("__Frozen__")then p:SetAttribute("__Frozen__",true)local r=g.PrimaryPart.CFrame;local s=r.LookVector*3;q.Anchored=true;q.CFrame=CFrame.new(r.Position+s,r.Position)*CFrame.Angles(0,math.rad(180),0)table.insert(j,q)table.insert(k,p)i[q]=p.Died:Connect(function()q.Anchored=false;p:SetAttribute("__Frozen__",false)j[q]=nil;k[p]=nil;i[q]:Disconnect()end)elseif p:GetAttribute("__Frozen__")then p:SetAttribute("__Frozen__",false)j[q]=nil;q.Anchored=false;i[q]:Disconnect()end end end end end;h.Died:Connect(function()for t,u in j do u.Anchored=false end;j={}for t,v in i do v:Disconnect()end;i={}for t,h in k do h:SetAttribute("__Frozen__",false)end;k={}end)script.Parent.Activated:Connect(m)end)
