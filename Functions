local a={}local function b()local c={}for d,e in game.Players:GetChildren()do if e.Character then table.insert(c,e.Character)end end;return c end;a.VisualizePosition=function(f)local g=Instance.new("Part")g.Position=f;g.CanCollide=false;g.Parent=workspace.Filter;g.Size=Vector3.new(0.1,0.1,0.1)g.Anchored=true end;a.GetDist=function(h,f)return(h["Right Arm"].Position-f).Magnitude end;a.GetClosestEnemy=function(h)local i=nil;local j=math.huge;for d,e in pairs(game.Players:GetChildren())do if e.Name~=h.Name and e.Character and e.Team~=game.Players[h.Name].Team then local k=(e.Character.PrimaryPart.Position-h.PrimaryPart.Position).Magnitude;if k<j then j=k;i=e.Character end end end;return i end;a.GetDistance=function(h,l)return(h.PrimaryPart.Position-l.PrimaryPart.Position).Magnitude end;a.GetClosestPart=function(f,l)local m=nil;local j=math.huge;for d,e in pairs(l:GetChildren())do if e:IsA("BasePart")then local n=Vector3.new(0,0,0)if e.Name=="Right Arm"then n=l.PrimaryPart.CFrame.LookVector*3 end;local k=(f-(e.Position+n)).Magnitude;if k<j then j=k;m=e end end end;return m end;a.GetRetreatDirection=function(h,i,k)local o=h.PrimaryPart.Position-i.PrimaryPart.Position;local p=RaycastParams.new()p.FilterType=Enum.RaycastFilterType.Blacklist;p.FilterDescendantsInstances=b()local q=-135;local r=0;local s=nil;local t=10;for d=1,t do local u=workspace:Raycast(h.PrimaryPart.Position,(CFrame.lookAt(h.PrimaryPart.Position,h.PrimaryPart.Position+o)*CFrame.Angles(0,math.rad(q),0)).LookVector*k,p)if u then local k=(u.Position-i.PrimaryPart.Position).Magnitude;if k>r then s=u.Position;r=k end end;q+=270/t end;return s end;a.SideCast=function(i)local p=RaycastParams.new()p.FilterType=Enum.RaycastFilterType.Blacklist;p.FilterDescendantsInstances=b()local q=0;local r=0;local s=nil;local t=10;for d=1,t do local u=workspace:Raycast(i.PrimaryPart.Position,(i.PrimaryPart.CFrame*CFrame.Angles(0,math.rad(q),0)).LookVector*8,p)q+=360/t;if u then return u end end end;return a
